{
  "name": "test-nn-ts",
  "version": "0.1.0",
  "description": "Test Neural Net in TypeScript",
  "main": "index.js",
  "author": "Wink Saville <wink@saville.com>",
  "license": "APACHE2",
  "scripts": {
    "add.nodev9": "rm -rf nodev9 && mkdir nodev9 && curl https://nodejs.org/download/v8-canary/v9.0.0-v8-canary201706199cf43ea3fd/node-v9.0.0-v8-canary201706199cf43ea3fd-linux-x64.tar.xz -s -o node-v9.tar.xz && tar -xJf node-v9.tar.xz -C nodev9 --strip-components 1 && rm node-v9.tar.xz",
    "nodev9": "./nodev9/bin/node",
    "cleanup": "rm -rf build; mkdir -p build",
    "distcleanup": "yarn cleanup && rm -rf node_modules nodev9",
    "postinstall": "yarn add.nodev9 && mkdir -p build",
    "build": "tsc -p src/tsconfig.json",
    "doit": "yarn nodev9 build/test-nn.js",
    "runit": "yarn build && yarn doit",
    "test": "yarn build && DEBUG=DBG yarn doit",
    "dbg": "yarn build && DEBUG=DBG node --inspect-brk build/test-nn.js"
  },
  "dependencies": {
    "@types/debug": "^0.0.29",
    "@types/microtime": "^2.1.0",
    "@types/node": "^7.0.27",
    "@types/numeral": "^0.0.21",
    "@types/seedrandom": "^2.4.27",
    "debug": "^2.6.8",
    "microtime": "^2.1.5",
    "numeral": "^2.0.6",
    "seedrandom": "^2.4.3",
    "typescript": "^2.3.4"
  }
}
